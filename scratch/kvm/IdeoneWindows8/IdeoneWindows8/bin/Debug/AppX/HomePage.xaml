<Page
    x:Class="IdeoneWindows8.HomePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:IdeoneWindows8"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{StaticResource ApplicationPageBackgroundThemeBrush}">
        <Image x:Name="picProfile" HorizontalAlignment="Left" Height="169" Margin="568,85,0,0" VerticalAlignment="Top" Width="184"/>
        <TextBlock x:Name="text1" HorizontalAlignment="Left" Margin="55,93,0,0" TextWrapping="Wrap" Text="UserName :" VerticalAlignment="Top" Height="26" Width="160" FontSize="20"/>
        <TextBlock x:Name="tbk_username" HorizontalAlignment="Left" Margin="241,93,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="26" Width="160" FontSize="20"/>
        <TextBlock x:Name="text1_Copy1" HorizontalAlignment="Left" Margin="55,124,0,0" TextWrapping="Wrap" Text="Full name :" VerticalAlignment="Top" Height="26" Width="160" FontSize="20"/>
        <TextBlock x:Name="text1_Copy2" HorizontalAlignment="Left" Margin="55,155,0,0" TextWrapping="Wrap" Text="Email :" VerticalAlignment="Top" Height="26" Width="160" FontSize="20"/>
        <TextBlock x:Name="text1_Copy3" HorizontalAlignment="Left" Margin="55,186,0,0" TextWrapping="Wrap" Text="User ID :" VerticalAlignment="Top" Height="26" Width="160" FontSize="20"/>
        <TextBlock x:Name="tbk_fullname" HorizontalAlignment="Left" Margin="241,124,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="26" Width="160" FontSize="20"/>
        <TextBlock x:Name="tbk_emailid" HorizontalAlignment="Left" Margin="241,155,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="26" Width="282" FontSize="20"/>
        <TextBlock x:Name="tbk_userid" HorizontalAlignment="Left" Margin="241,186,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="26" Width="160" FontSize="20"/>
        <Image x:Name="img_profile" HorizontalAlignment="Left" Height="127" Margin="537,85,0,0" VerticalAlignment="Top" Width="160"/>

    </Grid>
</Page>

